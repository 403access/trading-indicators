{
	"name": "app-mock-trade",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"format": "bunx biome format --write",
		"dev": "bun --hot ./src/apps/backend/main.ts",
		"start": "NODE_ENV=production bun src/index.tsx",
		"build": "bun run build.ts",
		"api:tradingpairs": "bun run ./src/scripts/getTradingPairs.ts",
		"api:tradingpairs:SOLUSDC": "bun run ./src/scripts/getTradingPairSOLUSDC.ts",
		"api:assetinfo": "bun run ./src/scripts/getAssetInfo.ts",
		"api:ohlcdata": "bun run ./src/scripts/getOHLCData.ts",
		"api:tradeshistory": "bun run ./src/scripts/get-trades-history.ts",
		"sync:full": "bun run ./src/scripts/kraken-sync.ts full",
		"sync:incremental": "bun run ./src/scripts/kraken-sync.ts incremental",
		"db:status": "bun run ./src/scripts/db-status.ts",
		"test:ratelimit": "bun run ./src/scripts/test-rate-limiter.ts",
		"test:refactor": "bun run ./src/scripts/test-refactor.ts",
		"test:env": "bun run ./src/scripts/test-env-helpers.ts",
		"test:performance": "bun run ./src/scripts/test-performance-refactor.ts"
	},
	"dependencies": {
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-slot": "^1.2.3",
		"bun-plugin-tailwind": "^0.0.15",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.525.0",
		"react": "^19",
		"react-day-picker": "^9.8.1",
		"react-dom": "^19",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/bun": "latest"
	}
}
